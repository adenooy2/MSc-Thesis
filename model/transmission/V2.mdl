{UTF-8}
mortality=
	untreated mortality rate*LTFU or incorrect diagnosis
	~	People/Year
	~		|

Patient Delay=
	DELAY1( Active,patient delay time )
	~	
	~		|

Diagnosed and On Treatment= INTEG (
	diagnosis-recovery,
		10000)
	~	
	~		|

diagnosis=
	Seeking care*CDR
	~	People/Year
	~		|

Diagnostic delay=
	DELAY1( LTFU or incorrect diagnosis, patient delay time )
	~	
	~		|

Diagnostic Loss=
	Seeking care*(1-CDR)
	~	People/Year
	~		|

Seeking care= INTEG (
	Diagnostic delay+Patient Delay-diagnosis-Diagnostic Loss,
		20000)
	~	
	~		|

infection=
	force of infection*((Susceptible*Active)/Total Pop)
	~	People/Year
	~		|

untreated mortality rate=
	0.5
	~	
	~		|

LTFU or incorrect diagnosis= INTEG (
	Diagnostic Loss-Diagnostic delay-mortality,
		20000*0.54)
	~	People
	~		|

patient delay time=
	14/365
	~	
	~		|

recovery rate=
	0.83
	~	
	~		|

Total Pop= INTEG (
	Active+Diagnosed and On Treatment+Latent TB infection+LTFU or incorrect diagnosis+Seeking care\
		+Susceptible,
		3.08516e+07)
	~	
	~		|

CDR=
	0.46+ RAMP(0.0383  , 2016 , 2022 )
	~	
	~		|

recovery=
	Diagnosed and On Treatment*recovery rate
	~	People/Year
	~		|

Active= INTEG (
	progression-Patient Delay,
		139000)
	~	People
	~		|

progression rate=
	0.08
	~	
	~		|

force of infection=
	0.02
	~	
	~		|

Susceptible= INTEG (
	recovery-infection,
		3.08516e+07)
	~	People
	~		|

progression=
	Latent TB infection*progression rate
	~	
	~		|

Latent TB infection= INTEG (
	infection-progression,
		300000)
	~	People
	~		|

Slow progression  = A FUNCTION OF( )
	~	
	~		|

